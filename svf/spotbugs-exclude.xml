        <Class name="gov.pnnl.svf.vbo.VertexBufferObjectImpl" />
        <Or>
            <Field name="colors" />
            <Field name="normals" />
            <Field name="texCoords" />
            <Field name="vertices" />
        </Or>
        <Bug code="EI" />
    </Match>
    <Match>
        <Class name="gov.pnnl.svf.vbo.VertexBufferObjectImpl" />
        <Or>
            <Field name="colors" />
            <Field name="normals" />
            <Field name="texCoords" />
            <Field name="vertices" />
        </Or>
        <Bug code="EI2" />
    </Match>
